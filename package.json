{
  "name": "@aberoth-community/overheard",
  "version": "2.2.2",
  "description": "A simple overheard scraper",
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "types": "./types/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "start": "ts-node src/index.ts",
    "clean": "rimraf bin dist",
    "build": "tsc",
    "build:dev": "tsc --sourceMap",
    "build:pkg": "pkg .",
    "lint": "eslint .",
    "format": "prettier -c --write \"**/*.{md,cjs,mjs,js,json,ts}\"",
    "format:check": "prettier --list-different \"**/*.{md,cjs,mjs,js,json,ts}\"",
    "prepare": "husky install",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aberoth-community/overheard.git"
  },
  "keywords": [
    "aberoth",
    "rpg"
  ],
  "author": "ashnel3",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/aberoth-community/overheard/issues"
  },
  "homepage": "https://github.com/aberoth-community/overheard#readme",
  "engines": {
    "node": ">=18"
  },
  "engineStrict": true,
  "pkg": {
    "outputPath": "bin"
  },
  "dependencies": {
    "commander": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "pkg": "^5.8.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
